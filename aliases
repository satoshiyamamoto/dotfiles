# ailiases
alias ls='exa'
alias l='ls -1a'
alias ll='ls -lgh --git --icons'
alias la='ll -a'
alias tree='exa -T'
alias k='kubectl'
alias vim='nvim'
alias view='vim -R'
alias gore='gore -autoimport'
alias vpn='/opt/cisco/anyconnect/bin/vpn'
alias grp='repo=$(ghq list | fzf) && cd $(ghq root)/$repo; unset repo'
alias ssh='TERM=xterm-256color ssh'

